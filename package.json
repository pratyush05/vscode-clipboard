{
    "name": "clipboard",
    "displayName": "Clipboard",
    "description": "Clipboard extension for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.76.0"
    },
    "publisher": "pratyush05",
    "author": {
        "name": "Pratyush Gupta",
        "email": "pratyush.gupta5594@gmail.com"
    },
    "preview": false,
    "private": true,
    "homepage": "https://github.com/pratyush05/vscode-clipboard/blob/main/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/pratyush05/vscode-clipboard.git"
    },
    "bugs": {
        "url": "https://github.com/pratyush05/vscode-clipboard/issues"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#702EA3",
        "theme": "dark"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "icon": "images/clipboardIcon.png",
    "contributes": {
        "commands": [
            {
                "command": "clipboard.copyFilename",
                "title": "Copy File Name"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "clipboard.copyFilename",
                    "when": "isFileSystemResource && resourceScheme == file"
                }
            ],
            "editor/title/context": [
                {
                    "command": "clipboard.copyFilename",
                    "when": "isFileSystemResource && resourceScheme == file",
                    "group": "1_cutcopypaste@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.76.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5",
        "@vscode/test-electron": "^2.2.3"
    }
}
